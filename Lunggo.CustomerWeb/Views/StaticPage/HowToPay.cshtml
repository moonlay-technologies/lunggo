@{
    Layout = "/Views/Shared/Desktop/_LayoutDesktop.cshtml";
    ViewBag.BodyClass = "page-static page-howtopay";
    ViewBag.Title = "Cara Bayar Tiket Pesawat dan Hotel di Travorama";
    ViewBag.ActiveSection = "pay";
    ViewBag.Page = "HowToPay";
}

@section AdditionalJS{
    <!-- additional Angular JS-->
}

@section PageJS{

    <script>
        // toggle list detail
        $('.list-summary .btn').click(function (evt) {
            evt.preventDefault();
            $(this).closest('.list').children('.list-detail').stop().slideToggle();
        });
        $('.close-list').click(function (evt) {
            evt.preventDefault();
            $(this).closest('.list').children('.list-detail').slideUp('active');
        });
    </script>

}

<section class="howto-header">
    <div class="line"><span></span></div>
    <hgroup>
        <h2>Metode Pembayaran di <b>Travorama</b></h2>
        <h4 class="md-txt">Mudah dan Cepat! Cukup 5 menit</h4>
    </hgroup>
</section><!-- .howto-header -->
@Html.Partial("/Views/StaticPage/_SectionHowTo.cshtml")

