@{
    Layout = "/Views/Shared/Travorama20/_Layout20150929.cshtml";
    ViewBag.BodyClass = "page-account page-account-profile";
    ViewBag.PageActiveSection = "order";
}

@section AdditionalJS{
    <!-- additional Angular JS-->
    <script src="/Assets/js/angular.min.js"></script>
    <script src="/Assets/js/angular-animate.min.js"></script>
    <script src="/Assets/js/angular-route.min.js"></script>
}

@section PageJS{
    <script>
        var langCode = 'en';
    </script>
    <!-- angular controller -->
    <script src="/Assets/travorama20/js/accountController.js"></script>
}


<div class="container">
    <div class="row">

        <div class="col-xs-3">
            @Html.Partial("/Views/Shared/travorama20/_SectionUserMenu.cshtml")
        </div>
        <!-- ******************** -->

        <div class="col-xs-9">
            <section class="account-content">

                <div class="order-detail">
                    <header class="content-header">
                        <h2>ORDER ID : <b>123456789012</b></h2>
                    </header>
                    <section class="content-body">
                        
                        <div class="info-buyer">
                            <header>
                                <h4><b>BUYER INFO</b> <a href="#" class="pull-right btn btn-yellow">cetak eTicket</a></h4>
                            </header>
                            <section>
                                <div class="form form-horizontal">
                                    <div class="form-group">
                                        <label class="control-label col-xs-3">User ID</label>
                                        <div class="col-xs-9">
                                            <p class="form-control-static">shinta@tamara.com</p>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-xs-3">Order Name</label>
                                        <div class="col-xs-9">
                                            <p class="form-control-static">Shinta Tamara</p>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-xs-3">Order Date</label>
                                        <div class="col-xs-9">
                                            <p class="form-control-static">15 Oktober 2015</p>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-xs-3">Order Type</label>
                                        <div class="col-xs-9">
                                            <p class="form-control-static">Flight</p>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-xs-3">Order Final Price</label>
                                        <div class="col-xs-9">
                                            <p class="form-control-static">Rp 999.999.999</p>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-xs-3">Payment Method</label>
                                        <div class="col-xs-9">
                                            <p class="form-control-static">ATM Transfer</p>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-xs-3">Payment Status</label>
                                        <div class="col-xs-9">
                                            <p class="form-control-static">Pending</p>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-xs-3">Penumpang</label>
                                        <div class="col-xs-9">
                                            <p class="form-control-static">Pending</p>
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div><!-- .info-buyer -->
                        
                        <div class="info-flight">
                            <header>
                                <h4><b>DETAIL PEMESANAN</b></h4>
                            </header>
                            <section>
                                <div class="flight departure">
                                    <p><img src="/Assets/images/airlines/AK.png" alt="AirAsia" /> <span>AIRASIA</span> </p>
                                    <div class="row">
                                        <div class="col-xs-2">
                                            <p>Berangkat : </p>
                                        </div>
                                        <div class="col-xs-9">
                                            <p>Jakarta (CGK) - Denpasar, Bali (DPS)</p>
                                            <p>Kamis, 15 Okt 2015</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="flight return">
                                    <p><img src="/Assets/images/airlines/AK.png" alt="AirAsia" /> <span>AIRASIA</span> </p>
                                    <div class="row">
                                        <div class="col-xs-2">
                                            <p>Berangkat : </p>
                                        </div>
                                        <div class="col-xs-9">
                                            <p>Jakarta (CGK) - Denpasar, Bali (DPS)</p>
                                            <p>Kamis, 15 Okt 2015</p>
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div><!-- .info-flight -->
                        
                        <div class="refund-detail">
                            <header>
                                <h4><b>DETAIL PEMESANAN</b></h4>
                            </header>
                            <section>
                                <div class="form form-horizontal">
                                    <div class="form-group">
                                        <label class="control-label col-xs-3">No Refund</label>
                                        <div class="col-xs-9">
                                            <p class="form-control-static">-</p>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-xs-3">Tgl Pengajuan</label>
                                        <div class="col-xs-9">
                                            <p class="form-control-static">-</p>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label col-xs-3">Status Refund</label>
                                        <div class="col-xs-9">
                                            <p class="form-control-static">-</p>
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div><!-- .refund-detail -->

                    </section>
                </div><!-- .history-content -->

            </section><!-- .account-content -->
        </div>

    </div>
</div>