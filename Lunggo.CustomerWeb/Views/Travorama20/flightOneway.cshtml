    Layout = "/Views/Shared/Travorama20/_Layout20150929.cshtml";
    ViewBag.BodyClass = "page-flight page-flight-oneway";
}

@section PageJS{
    <script>

        searchParam = {};

        flightPageFunctions();
    </script>
