<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <link rel="shortcut icon" type="image/ico" href="/Assets/images/favico.ico" />

    @{ Html.RenderPartial("_openGraph"); }
    @{ Html.RenderPartial("_metaTag"); }

    <title>@ViewBag.Title | Travorama B2B</title>
    


    <!-- stylesheets -->
    <link href="/Assets/B2BTravorama/assets/plugins/pace/pace-theme-flash.css" rel="stylesheet" type="text/css" />
    <link href="/Assets/B2BTravorama/assets/plugins/bootstrapv3/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="/Assets/B2BTravorama/assets/plugins/font-awesome/css/font-awesome.css" rel="stylesheet" type="text/css" />
    <link href="/Assets/B2BTravorama/assets/plugins/jquery-scrollbar/jquery.scrollbar.css" rel="stylesheet" type="text/css" media="screen" />
    <link href="/Assets/B2BTravorama/assets/plugins/select2/css/select2.min.css" rel="stylesheet" type="text/css" media="screen" />
    <link href="/Assets/B2BTravorama/assets/plugins/switchery/css/switchery.min.css" rel="stylesheet" type="text/css" media="screen" />
    <link href="/Assets/B2BTravorama/assets/plugins/bootstrap3-wysihtml5/bootstrap3-wysihtml5.min.css" rel="stylesheet" type="text/css" />
    <link href="/Assets/B2BTravorama/assets/plugins/bootstrap-tag/bootstrap-tagsinput.css" rel="stylesheet" type="text/css" />
    <link href="/Assets/B2BTravorama/assets/plugins/dropzone/css/dropzone.css" rel="stylesheet" type="text/css" />
    <link href="/Assets/B2BTravorama/assets/plugins/bootstrap-datepicker/css/datepicker3.css" rel="stylesheet" type="text/css" media="screen">
    <link href="/Assets/B2BTravorama/assets/plugins/summernote/css/summernote.css" rel="stylesheet" type="text/css" media="screen">
    <link href="/Assets/B2BTravorama/assets/plugins/bootstrap-daterangepicker/daterangepicker-bs3.css" rel="stylesheet" type="text/css" media="screen">
    <link href="/Assets/B2BTravorama/assets/plugins/bootstrap-timepicker/bootstrap-timepicker.min.css" rel="stylesheet" type="text/css" media="screen">
    <link href="/Assets/B2BTravorama/pages/css/pages-icons.css" rel="stylesheet" type="text/css">
    <link class="main-stylesheet" href="/Assets/B2BTravorama/pages/css/pages.css" rel="stylesheet" type="text/css" />
    <link href="/Assets/css/style_b2b.css" rel="stylesheet" />


   
    @if (IsSectionDefined("AdditionalCSS"))
    {
        @RenderSection("AdditionalCSS");
    }

    @section AdditionalCSS{
    }

    @section AdditionalJS{
    }

    @section PageJS{

    }

</head>
<body class="fixed-header @ViewBag.BodyClass">
    @Html.Partial("/Views/Shared/_googleTag.cshtml")

    @if (ViewBag.SiteHeader != "b2b-login" && ViewBag.SiteHeader != "b2b-reset-password" && ViewBag.SiteHeader != "b2b-confirm-password")
    {
        @Html.Partial("/Views/Shared/B2BTravorama/_B2BSideMenu.cshtml")
        <div class="page-container">
            @Html.Partial("/Views/Shared/B2BTravorama/_B2BHeaderMenu.cshtml")
            <div class="page-content-wrapper">
                <div class="content">
                    @if (ViewBag.SiteHeader != "b2b-order-list" && ViewBag.SiteHeader != "b2b-user-manage" && ViewBag.SiteHeader != "b2b-order-detail")
                    {
                        <div class="container container-fluid">
                            @RenderBody()
                        </div>
                    }
                    else
                    {
                        <div class="container-fluid">
                            @RenderBody()
                        </div>
                    }
                </div>

            </div>
        </div>
    }
    else
    {
        <div class="login-wrapper ">
            <div class="bg-pic"></div>
            <div class="login-container bg-white">
                <div class="p-l-50 m-l-20 p-r-50 m-r-20 p-t-50 m-t-30 sm-p-l-15 sm-p-r-15 sm-p-t-40">
                    <img src="/Assets/B2BTravorama/assets/img/logo.png" alt="logo" data-src="/Assets/B2BTravorama/assets/img/logo.png" width="78" height="22">
                    @RenderBody()
                </div>
            </div>
        </div>
    }

    <script src="/Assets/B2BTravorama/assets/plugins/pace/pace.min.js" type="text/javascript"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/jquery/jquery-1.11.1.min.js" type="text/javascript"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/modernizr.custom.js" type="text/javascript"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/jquery-ui/jquery-ui.min.js" type="text/javascript"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/bootstrapv3/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/jquery/jquery-easy.js" type="text/javascript"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/jquery-unveil/jquery.unveil.min.js" type="text/javascript"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/jquery-bez/jquery.bez.min.js"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/jquery-ios-list/jquery.ioslist.min.js" type="text/javascript"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/jquery-actual/jquery.actual.min.js"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/jquery-scrollbar/jquery.scrollbar.min.js"></script>
    <script type="text/javascript" src="/Assets/B2BTravorama/assets/plugins/select2/js/select2.full.min.js"></script>
    <script type="text/javascript" src="/Assets/B2BTravorama/assets/plugins/classie/classie.js"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/switchery/js/switchery.min.js" type="text/javascript"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/bootstrap3-wysihtml5/bootstrap3-wysihtml5.all.min.js"></script>
    <script type="text/javascript" src="/Assets/B2BTravorama/assets/plugins/jquery-autonumeric/autoNumeric.js"></script>
    <script type="text/javascript" src="/Assets/B2BTravorama/assets/plugins/dropzone/dropzone.min.js"></script>
    <script type="text/javascript" src="/Assets/B2BTravorama/assets/plugins/bootstrap-tag/bootstrap-tagsinput.min.js"></script>
    <script type="text/javascript" src="/Assets/B2BTravorama/assets/plugins/jquery-inputmask/jquery.inputmask.min.js"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/bootstrap-form-wizard/js/jquery.bootstrap.wizard.min.js" type="text/javascript"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/jquery-validation/js/jquery.validate.min.js" type="text/javascript"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js" type="text/javascript"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/summernote/js/summernote.min.js" type="text/javascript"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/moment/moment.min.js"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/bootstrap-daterangepicker/daterangepicker.js"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/bootstrap-timepicker/bootstrap-timepicker.min.js"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/bootstrap-typehead/typeahead.bundle.min.js"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/bootstrap-typehead/typeahead.jquery.min.js"></script>
    <script src="/Assets/B2BTravorama/assets/plugins/handlebars/handlebars-v4.0.5.js"></script>

    <script src="/Assets/B2BTravorama/pages/js/pages.js"></script>
    <script src="/Assets/B2BTravorama/assets/js/form_elements.js" type="text/javascript"></script>
    <script src="/Assets/B2BTravorama/assets/js/scripts.js" type="text/javascript"></script>
    <script src="/Config/JsConfig.js"></script>

    @if (IsSectionDefined("AdditionalJS"))
    {
        @RenderSection("AdditionalJS");
    }

    @if (IsSectionDefined("PageJS"))
    {
        @RenderSection("PageJS");
    }




</body>
</html>