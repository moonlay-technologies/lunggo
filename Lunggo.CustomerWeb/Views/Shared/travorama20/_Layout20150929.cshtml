<body class="@ViewBag.BodyClass">
    @Html.Partial("/Views/Shared/_googleTag.cshtml")

    <!--********************-->
    @Html.Partial("/Views/Shared/travorama20/_SiteHeader.cshtml")
    
    <!--********************-->
    <section class="site-content">
        
        @RenderBody()

    </section><!-- .site-content -->
    
    <!--********************-->
    <!-- footer group start -->
    @Html.Partial("/Views/Shared/travorama20/_SiteFooterMenu.cshtml")
    @Html.Partial("/Views/Shared/travorama20/_SiteFooter.cshtml")
   
    <!--********************-->
    <!-- Javascripts -->
    <script src="/Assets/js/jquery.min.js"></script>
    <script src="/Assets/travorama20/js/bootstrap.min.js"></script>
    <script src="/Assets/js/jquery-ui.min.js"></script>
    <script src="/Config/JsConfig.js"></script>

    @if (IsSectionDefined("AdditionalJS"))
    {
        @RenderSection("AdditionalJS");
    }
    
    <script src="/Assets/travorama20/js/travorama20.js"></script>

    @if (IsSectionDefined("PageJS"))
    {
        @RenderSection("PageJS");
    }
    
    <script type="text/javascript">
    $(function () {
        if (/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent)) {
            var ww = ($(window).width() < window.screen.width) ? $(window).width() : window.screen.width;
            var mw = 1000;
            var ratio = ww / mw;
            if (ww < mw) {
                $('#viewport').attr('content', 'initial-scale=' + ratio + ', minimum-scale=' + ratio + ', user-scalable=yes, width=' + ww);
            } else {
                $('#viewport').attr('content', 'initial-scale=1, maximum-scale=2, minimum-scale=1.0, user-scalable=yes, width=' + ww);
            }
        }
    });
    </script>

    <div class="page-newsletter">
        <div class="overlay-content">
            <section>
                <p class="header"><img src="~/Assets/images/popup/header1.png"/></p>
                <p class="popup-text"><span style="color:#ffffff">Dapatkan informasi harga tiket murah dengan</span><span style="color:#ffd632"> <b>mendaftarkan email Anda</b></span></p>
                <span class="email"><input class="input-type" type="email" placeholder="Masukkan Email Anda"></span>
                <button class="btn btn-yellow">SUBMIT</button>
            </section>
        </div>
    </div>
