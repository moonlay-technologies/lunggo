                            <input type="submit" name="activitySubmit" value="SEARCH" class="btn btn-yellow block form-control" />
                        </div>
                    </form>
                </div><!-- .tab-activity -->
            </section>

        </div><!-- .search-form -->
        @Html.Partial("/Views/Shared/Travorama20/_SearchCalendar.cshtml")
