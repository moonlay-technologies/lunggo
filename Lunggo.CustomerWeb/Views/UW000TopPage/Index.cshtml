                        <h3 class="text-center">Cari <b>Tiket Pesawat</b></h3>

                        <div class="form-group flight-type">
                            <div class="col-xs-6">
                                <label>
                                    <input type="radio" name="flightType" class="form-flight-type" value="return" checked />
                                    <span>Pulang Pergi</span>
                                </label>
                            </div>
                            <div class="col-xs-6">
                                <label>
                                    <input type="radio" name="flightType" class="form-flight-type" value="oneway" />
                                    <span>Sekali Jalan</span>
                                </label>
                            </div>
                        </div>

                        <div class="form-group flight-destination">
                            <div class="col-xs-12">
                                <input type="text" class="form-flight-origin form-flight-location form-control" value="" placeholder="Kota Asal" />
                            </div>
                            <div class="text-center col-xs-12">
                                <span class="switch-destination"></span>
                            </div>
                            <div class="col-xs-12">
                                <input type="text" class="form-flight-destination form-flight-location form-control" value="" placeholder="Kota Tujuan" />
                            </div>
                        </div>

                        <div class="form-group flight-date">
                            <div class="col-xs-6">
                                <div class="form-flight-departure">
                                    <small>Tanggal Keberangkatan</small>
                                    <span class="date">28</span>
                                    <span class="month">Jul</span>
                                    <span class="year">2015</span>
                                </div>
                            </div>
                            <div class="col-xs-6">
                                <div class="form-flight-return">
                                    <small>Tanggal Pulang</small>
                                    <span class="date">28</span>
                                    <span class="month">Jul</span>
                                    <span class="year">2015</span>
                                </div>
                                <div class="form-flight-oneway">
                                    <small>Tanggal Pulang</small>
                                    <h5>SEKALI JALAN</h5>
                                </div>
                            </div>
                        </div>

                        <div class="form-group flight-class">
                            <div class="col-xs-12">
                                <div class="form-control text-center form-flight-class">
                                    <span>Kelas Ekonomi</span>
                                    <div class="option">
                                        <span class="economy" data-value="y">Kelas Ekonomi</span>
                                        <span class="business" data-value="c">Kelas Bisnis</span>
                                        <span class="first" data-value="f">Kelas Utama</span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="form-group flight-passenger">
                            <div class="col-xs-4">
                                <div class="form-flight-passenger adult" data-toggle="tooltip" data-placement="left" title="Tidak boleh kosong">
                                    <h5 class="passenger-input adult">1</h5>
                                    <small>
                                        Dewasa<br />(&gt;12th)
                                    </small>
                                    <div class="option">
                                        <span>1</span>
                                        <span>2</span>
                                        <span>3</span>
                                        <span>4</span>
                                        <span>5</span>
                                        <span>6</span>
                                        <span>7</span>
                                        <span>8</span>
                                        <span>9</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-4">
                                <div class="form-flight-passenger child">
                                    <h5 class="passenger-input child">0</h5>
                                    <small>
                                        Anak<br />(2-12th)
                                    </small>
                                    <div class="option">
                                        <span>0</span>
                                        <span>1</span>
                                        <span>2</span>
                                        <span>3</span>
                                        <span>4</span>
                                        <span>5</span>
                                        <span>6</span>
                                        <span>7</span>
                                        <span>8</span>
                                        <span>9</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-4">
                                <div class="form-flight-passenger infant" data-toggle="tooltip" data-placement="right" title="Tidak boleh lebih dari jumlah penumpang dewasa">
                                    <h5 class="passenger-input infant">0</h5>
                                    <small>
                                        Bayi<br />(&lt;2th)
