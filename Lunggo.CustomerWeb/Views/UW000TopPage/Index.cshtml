                                <p>Harga mulai dari <b>Rp {{destination.price | number}}</b></p>
                                <a class="btn btn-yellow" href="@Url.Action("Search","Flight")?info={{ destination.origin + '' + flightDestination.code + '' + departureDate  }}-100y">LIHAT SEKARANG</a>
                            </section>
                        </div>
                    </div>

                </div>    

            </div><!-- .destination-wrapper -->

        </div>
    </section>
}

