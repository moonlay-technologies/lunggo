<div class="page-dashboard">
    <div class="page-modal">
        <div class="panel panel-transparent">
            <div class="panel-heading">
                <div class="panel-title">
                    Modal Box Notification Example
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="panel-body">
                <div class="">
                    <button class="btn btn-primary btn-cons btn-modal" data-toggle="modal" data-target=".modal-sm-double" data-backdrop="static" data-keyboard="false">Modal 2 Btn</button>
                    <span class="m-r-20"></span>
                    <button class="btn btn-primary btn-cons btn-modal" data-toggle="modal" data-target=".modal-sm-single" data-backdrop="static" data-keyboard="false">Modal 1 Btn</button>
                </div>
                <div class="m-t-20">
                    <button class="btn btn-primary btn-cons btn-modal" data-toggle="modal" data-target=".modal-sm-default">Modal No-Btn</button>
                    <span class="m-r-20"></span>
                    <button class="btn btn-primary btn-cons btn-modal" data-toggle="modal" data-target=".modal-sm-default-close" data-backdrop="static" data-keyboard="false">Modal No-Btn Close</button>
                </div>

            </div>
        </div>
        <!-- Example Modal Box -->

    </div>
    <!-- With 2 buttons -->
    <div class="modal fade slide-up disable-scroll modal-sm-double" tabindex="-1" role="dialog" aria-hidden="false">
        <div class="modal-dialog modal-sm">
            <div class="modal-content-wrapper">
                <div class="modal-content">
                    <div class="modal-header clearfix">
                        <h4 class="text-center no-margin">Title</h4>
                    </div>
