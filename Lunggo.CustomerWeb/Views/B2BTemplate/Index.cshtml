                        </div>
                        <button type="button" class="btn btn-primary btn-cons" data-dismiss="modal">Ok</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- With no buttons -->
    <div class="modal fade slide-up disable-scroll modal-sm-default" tabindex="-1" role="dialog" aria-hidden="false">
        <div class="modal-dialog modal-sm">
            <div class="modal-content-wrapper">
                <div class="modal-content">
                    <div class="modal-header clearfix">
                        <h4 class="text-center no-margin">Title</h4>
                    </div>
