@model IEnumerable<Lunggo.BackendWeb.Model.SubscriberCalendar>

@{
    ViewBag.Title = "SubscriberList";
}

<h3><i class="fa fa-angle-right"></i> Subscriber List</h3>
<div class="row">

    <div class="col-md-12">
        <div class="content-panel">
            <h4><i class="fa fa-angle-right"></i> Subscriber List</h4>
            <hr>
            <table class="table table-hover table-flight-booking">
                <thead>
                    <tr>
                        <th>No</th>
                        <th>Email</th>
                        <th>Name</th>
                        <th>Phone Number</th>
                        <th>City</th>
                        <th>Address</th>
                        <th>Postal Code</th>
                    </tr>
                </thead>
                <tbody>
                    @{var count = 1;}
                    @foreach (var rsv in Model)
                    {
                        <tr>
                            <td>
                                @count
                            </td>
                            <td>
                                @Html.DisplayFor(m => rsv.Email)
                            </td>
                            <td>
                                @Html.DisplayFor(m => rsv.Name)
                            </td>
                            <td>
                               @Html.DisplayFor(m => rsv.PhoneNumber)
                            </td>
                            <td>
                                @Html.DisplayFor(m => rsv.City)
                            </td>
                            <td>
                                @Html.DisplayFor(m => rsv.Address)
                            </td>
                            <td>
                                @Html.DisplayFor(m => rsv.PostalCode)
                            </td>
                            @{
                                count += 1;
                            }
                        </tr>
                    }

                </tbody>
            </table>
        </div><!--/content-panel -->
    </div><!-- /col-md-12 -->

</div><!-- row -->

